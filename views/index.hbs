<!-- Call to Action Well -->
<div class="row">
    <div class="col-lg-12">
        <div class="well text-center">
            Available Papers
        </div>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

{{#each papers}}
    {{#if start}}
        <!-- Content Row -->
        <div class="row">
    {{/if}}
        <div class="col-md-3">
            <div class="panel panel-default">
                <div class="panel-heading"><h2>{{paper.title}}</h2></div>
                <div class="panel-body"><p>{{paper.abstract}}</p></div>
            </div>
        </div>
        <!-- /.col-md-3 -->
    {{#if end }}
        </div>
        <!-- /.row -->
    {{/if}}
{{/each}}